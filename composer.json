{
    "name": "maff/zend-1-psr-7-bridge",
    "description": "Zend Framework 1 to PSR-7 bridge",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Geat",
            "email": "mathias@ailoo.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Maff\\Zend1MvcPsrMessageBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maff\\Zend1MvcPsrMessageBridge\\Test\\": "test/"
        }
    },
    "require": {
        "psr/http-message": "^1.0",
        "zendframework/zendframework1": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "psr/http-message-implementation": "To use the HttpFoundation factory",
        "zendframework/zend-diactoros": "To use the Zend Diactoros factory"
    }
}
